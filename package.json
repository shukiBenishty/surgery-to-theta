    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.11",
    "firebase": "^5.3.1",
    "firebase-admin": "^6.0.0",
    "glamor": "^2.20.40",
    "material-ui": "^1.0.0-beta.44",
    "moment": "^2.22.2",
    "react": "^16.3.2",
    "react-datetime": "^2.14.0",
    "react-dom": "^16.3.2",
